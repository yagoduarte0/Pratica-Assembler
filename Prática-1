; Prática 1 - Assembly

; Elaborar um programa em assembly para armazenar os 4 primeiros números pares nos registradores 0 a 3 do processador do Atmega2560, a partir do número 8. Usar somente representação numérica decimal.


inicio:
	ldi r16,0x8
	mov r0,r16
	mov r1,r0
	inc r1
	inc r1
	mov r2,r1
	inc r2
	inc r2
	mov r3,r2
	inc r3
	inc r3
	break
